{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/components/*":["src/components/*"],
      "@/data/*":["src/data/*"],
      "@/helpers/*":["src/helpers/*"],
      "@/hooks/*":["src/hooks/*"],
      "@/hooks":["src/hooks"],
      "@/layouts/*":["src/layouts/*"],
      "@/layouts":["src/layouts"],
      "@/types/*":["src/types/*"],
      "@/types":["src/types"],
      "@/store/*":["src/store/*"],
      "@/apis":["src/apis"],

    },
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
